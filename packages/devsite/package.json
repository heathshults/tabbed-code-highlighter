{
  "name": "hscript-wc-pwa",
  "version": "1.0.0",
  "description": "Progressive web app for housing my web components",
  "main": "index.js",
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "babel": "babel --presets @babel/preset-env js/main.js -o public/main.bundle.js",
    "server.test": "cd devsite && http-server -p 9304",
    "site.start": "cd devsite && .bin/hugo server",
    "site.alt.dev": "cd devsite && .bin/hugo server --gc --baseURL http://localhost/ --port 1313 --disableLiveReload --noHTTPCache --navigateToChanged --meminterval ms --memstats ../logs --log --logFile ./_logs/hugo.log --verbose --verboseLog",
    "site.build": "cd devsite && .bin/hugo",
    "page.create": "cd devsite && .bin/hugo new",
    "idxdb": "cd devsite && hugo-lunr  -i \"src/content/**/*\" -o src/content/search/site-index.json",
    "eslint": "cd devsite && eslint -c ./config/.eslintrc",
    
  },
  "keywords": [
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "prismjs"
  ],
  "author": "Heath Shults",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-parameters": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@csstools/normalize.css": "^9.0.1",
    "@types/prismjs": "^1.9.0",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "clipboard": "^2.0.4",
    "global": "^4.3.2",
    "http-server": "^0.11.1",
    "hugo-lunr": "0.0.4",
    "jsbeautify": "^0.3.6",
    "list-stream": "^1.0.1",
    "markdown-it": "^8.4.2",
    "marked": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prismjs": "^1.15.0",
    "puppeteer": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "slimerjs": "^1.0.0",
    "stylelint": "^9.10.1",
    "stylelint-order": "^2.1.0",
    "webpack": "^4.32.2"
  },
  "eslint.options": {
    "configFile": "./.eslintrc.js"
  },
  "configDir": "./config",
  "repository": {
    "type": "git",
    "url": "ssh+https://github.com/heathshults/tabbed-code-highlighter.git"
  },
  "homepage": "https://heathshults.com"
}
