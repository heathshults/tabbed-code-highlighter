{
  "name": "framekit-highlight-code",
  "version": "1.0.0",
  "description": "A Web Component to highlight your code",
  "unpkg":"dist/ionic.js",
  "module": "web_components/dist/esm/index.js",
  "main": "web_components/dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "hugo-bin": {
    "buildTags": "extended"
  },
  "scripts": {
    "stencil.build": "cd packages/web_components && stencil build --docs",
    "start": "cd packages/web_components && stencil build --dev --watch --serve",
    "stencil.test": "cd packages/web_components && stencil test --spec --e2e",
    "test.watch": "cd packages/web_components && stencil test --spec --e2e --watchAll",
    "babel": "babel --presets @babel/preset-env packages/javascript/src/main.js -o packages/devsite/static/js/main.bundle.js",
    "server.test": "cd packages/devsite && http-server -p 9304",
    "site.start": "cd packages/devsite && .bin/hugo server",
    "site.alt.dev": "cd packages/devsite && .bin/hugo server --gc --baseURL http://localhost/ --port 1313 --disableLiveReload --noHTTPCache --navigateToChanged --meminterval ms --memstats ../logs --log --logFile ./_logs/hugo.log --verbose --verboseLog",
    "site.build": "cd packages/devsite && .bin/hugo",
    "page.create": "cd packages/devsite && .bin/hugo new",
    "idxdb": "cd packages/devsite && hugo-lunr  -i \"src/content/**/*\" -o src/content/search/site-index.json",
    "eslint": "cd packages/devsite && eslint -c ./config/.eslintrc"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-parameters": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@csstools/normalize.css": "^9.0.1",
    "@stencil/core": "^0.18.0",
    "@stencil/postcss": "^0.1.0",
    "@stencil/sass": "^0.1.1",
    "@stencil/utils": "0.0.5",
    "@types/prismjs": "^1.9.0",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "clipboard": "^2.0.4",
    "global": "^4.3.2",
    "http-server": "^0.11.1",
    "hugo-lunr": "0.0.4",
    "jsbeautify": "^0.3.6",
    "lerna": "^3.16.4",
    "list-stream": "^1.0.1",
    "markdown-it": "^8.4.2",
    "marked": "^0.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "prismjs": "^1.15.0",
    "puppeteer": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "slimerjs": "^1.0.0",
    "stylelint": "^9.10.1",
    "stylelint-order": "^2.1.0",
    "webpack": "^4.32.2"
  },
  "collection": "dist/collection/collection-manifest.json",
  "eslint.options": {
    "configFile": "./.eslintrc.js"
  },
  "configDir": "./config",
  "repository": {
    "type": "git",
    "url": "ssh+https://github.com/heathshults/tabbed-code-highlighter.git"
  },
  "author": "Heath Shults",
  "license": "MIT",
  "bugs": {
    "url": "hhttps://github.com/heathshults/tabbed-code-highlighter/bugs"
  },
  "homepage": "https://heathshults.com",
  "keywords": [
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "presentation",
    "slides",
    "slideshow",
    "talk",
    "prismjs"
  ]
}
